<Window x:Class="Lab_Project_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab_Project_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="GetInfo" Content="Get Your Speeding Status" HorizontalAlignment="Center" VerticalAlignment="Top" Click="ButtonClick" Width="148" Height="50" Margin="0,217,0,0" />
        <Label Content="Speed: " HorizontalAlignment="Left" VerticalAlignment="Top" Width="48" Height="27" Margin="184,84,0,0" />
        <Label Content="Speed Limit: " HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Height="27" Margin="157,157,0,0"/>
        <TextBox x:Name="Speed" HorizontalAlignment="Center" Margin="0,84,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="148" Height="27"/>
        <TextBox x:Name="SpeedLimit" HorizontalAlignment="Center" Margin="0,157,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="148" Height="26"/>
        
    </Grid>
</Window>
